<?xml version="1.0" encoding="UTF-8"?>
<persistence version="2.0"
	xmlns="http://java.sun.com/xml/ns/persistence">
	<persistence-unit name="pole" transaction-type="JTA">
		<jta-data-source>jdbc/__default</jta-data-source>
		<provider>org.eclipse.persistence.jpa.PersistenceProvider</provider>
		<property name="eclipselink.ddl-generation" value="create-tables" />
		<property name="eclipselink.logging.level" value="INFO" />
		<property name="javax.persistence.jdbc.driver" value="org.h2.Driver" />
		<class>model.Affectation</class>
		<class>model.AffectationPK</class>
		<class>model.Cour</class>
		<class>model.Candidat</class>
		<class>model.Contenu</class>
		<class>model.Item</class>
		<class>model.Master</class>
		<class>model.Personne</class>
		<class>model.Preference</class>
		<class>model.PreferencePK</class>
		<class>model.Role</class>
		<class>model.Session</class>
		<class>model.AffectBi</class>
		<class>model.SvAffect</class>
	</persistence-unit>
</persistence>
